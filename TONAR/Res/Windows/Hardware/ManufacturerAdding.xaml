<Window x:Class="TONAR.Res.Windows.Hardware.ManufacturerAdding"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TONAR.Res.Windows.Hardware"
        mc:Ignorable="d"
        Title="Добавить производитея" Height="120" Width="211"
        ResizeMode="NoResize"
        ShowInTaskbar="False">
    <Grid>
        <Label Content="Введите номер процессора"></Label>
        <TextBox x:Name="tbName" Margin="5,30,0,0" Width="185" MaxLength="8"></TextBox>
        <Button Click="btnAdd_Click" x:Name="btnAdd" Content="Добавить" Width="90" Margin="5,55,5,5"></Button>
        <Button Click="btnCancel_Click" x:Name="btnCancel" Content="Отмена" Width="90" Margin="100,55,5,5"></Button>
    </Grid>
</Window>
